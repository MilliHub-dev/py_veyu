{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel_app.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/vercel_app"
    }
  ]
}